%h3 User Login

= form_tag sessions_path do 
  %p
    = label_tag :email
    = text_field_tag :email, params[:email]
  %p
    = label_tag :password
    = password_field_tag :password

  = submit_tag "Log in", :class => "btn btn-primary"
  = link_to "Sign Up for a new account", sign_up_path , :class => "btn btn-link"

